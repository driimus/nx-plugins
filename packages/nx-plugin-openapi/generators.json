{
  "$schema": "http://json-schema.org/schema",
  "name": "nx-plugin-openapi",
  "version": "0.0.0-semantically-released",
  "generators": {
    "init": {
      "factory": "./src/generators/init/generator",
      "schema": "./src/generators/init/schema.json",
      "description": "Initialize the @driimus/nx-plugin-openapi plugin",
      "aliases": ["ng-add"],
      "hidden": true
    },
    "api-spec": {
      "factory": "./src/generators/api-spec/generator",
      "schema": "./src/generators/api-spec/schema.json",
      "description": "Create a lib for an OpenAPI specification file"
    },
    "api-lib": {
      "factory": "./src/generators/api-lib/generator",
      "schema": "./src/generators/api-lib/schema.json",
      "description": "Create a lib to generate sources from an OpenAPI specification file"
    }
  }
}
